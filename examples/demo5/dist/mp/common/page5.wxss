
page {
  background-color: #eee;
}
.h5-button {
  display: block;
  width: 100%;
  height: 30px;
  line-height: 30px;
  text-align: center;
  font-size: 20px;
  border: 1px solid #ddd;
  background-color: #fff;
  margin-bottom: 20px;
}
.cnt {
  margin-top: 20px;
}
.item {
  display: flex;
  background-color: #fff;
  border-radius: 20px;
  margin: 0 20px 20px;
}
.head {
  margin: 20px;
  width: 80px;
  height: 80px;
  background-color: #ccc;
  color: #fff;
  border-radius: 50%;
  font-size: 16px;
  text-align: center;
  line-height: 80px;
}
.green {
  background-color: green;
}
.red {
  background-color: red;
}
.info {
  flex: 1;
  padding: 30px 30px 20px 5px;
}
.line {
  height: 15px;
  background-color: #eee;
  margin-bottom: 10px;
}

